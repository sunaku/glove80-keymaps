        #binding-cells = <2>;
        bindings = <&mo>, <&kp>;
    };
    thumb_parang_left: miryoku_thumb_layer_on_parang_left {
        compatible = "zmk,behavior-hold-tap";
        label = "MIRYOKU_THUMB_LAYER_ON_PARANG_LEFT";
