combos {
  compatible = "zmk,combos";

  //
  // COMBO_FIRING_DECAY defines how much time you have left (milliseconds)
  // after pressing the first key in a ZMK combo to finish the whole chord.
  //
  #ifndef COMBO_FIRING_DECAY
  #define COMBO_FIRING_DECAY 50
  #endif

  //
  // SHIFT_FIRING_DECAY defines how much time you have left (milliseconds)
  // after pressing the first key in the one-shot sticky shift ZMK combo
  // (on index finger home + thumb cluster T1) to finish the whole chord.
  //
  #ifndef SHIFT_FIRING_DECAY
  #define SHIFT_FIRING_DECAY 100
  #endif

  // T2+T3

  combo_sticky_globe_left {
    timeout-ms = <COMBO_FIRING_DECAY>;
    key-positions = <POS_LH_T2 POS_LH_T3>;
    bindings = <&sk _GLOBE>;
    layers = <0 1 2 3 4 5 6>;
